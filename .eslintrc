{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.dev.js"
      }
    }
  },
  "ecmaFeatures": {
    "generators": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "spaced-comment": 0,
    "func-names": 0,
    "no-console": 0,
    "eqeqeq": 0,
    "id-length": 0,
    "no-nested-ternary": 1,
    "no-use-before-define": 0,
    "space-before-function-paren": 0,
    "max-len": 0,
    "consistent-return": 0,
    "newline-per-chained-call": 0,
    "global-require": 0
  }
}
